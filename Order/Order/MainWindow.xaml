<Window x:Name="Main" x:Class="Order.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Order"
        mc:Ignorable="d"
        Title="Order" Width="1280" Height="720" MinWidth="1280" MinHeight="720" VerticalAlignment="Top" FontSize="24" ResizeMode="NoResize" WindowStartupLocation="CenterScreen"  >
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1000" MinWidth="1000"/>
            <ColumnDefinition Width="280" MinWidth="280"/>
        </Grid.ColumnDefinitions>

        <Frame x:Name="FrameForPages" HorizontalAlignment="Left" Height="690" VerticalAlignment="Top" Width="1000" NavigationUIVisibility="Hidden"/>
        <Frame x:Name="FrameForPages2" Grid.Column="1" Margin="5,0,10,0.4" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
